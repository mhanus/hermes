<?xml version="1.0" encoding="utf-8"?>
<mesh:mesh xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:mesh="XMLMesh"
  xmlns:element="XMLMesh"
  xsi:schemaLocation="XMLMesh ../../xml_schemas/mesh_h2d_xml.xsd">
  
  <vertices>
    <vertex x="0" y="-1" i="0"/>
    <vertex x="0" y="0" i="2"/>
    <vertex x="0" y="1" i="4"/>
	<vertex x="1" y="-0.5" i="1"/>
    <vertex x="1" y="0.5" i="3"/>
	<vertex x="-0.3" y="0.7" i="5"/>
  </vertices>

  <elements>
    <element:triangle v1="0" v2="1" v3="4" marker="Aluminum" />
	<element:triangle v1="0" v2="4" v3="5" marker="Aluminum" />
  </elements>

  <edges>
    <edge v1="0" v2="1" marker="Bnd" />
    <edge v1="1" v2="4" marker="Bnd" />
	<edge v1="4" v2="5" marker="Bnd" />
    <edge v1="5" v2="0" marker="Bnd" />
  </edges>
  <curves>
    <arc v1="4" v2="5" angle="50" />
  </curves>
</mesh:mesh>